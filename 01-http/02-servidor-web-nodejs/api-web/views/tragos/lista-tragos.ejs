


<table>
    <tr>
        <th>Nombre</th>
        <th>Grados</th>
        <th>Caduca</th>
        <th>Precio</th>
        <th>Tipo</th>
        <th>Acciones</th>
    </tr>
    <%arregloTragos.forEach((trago)=>{%>
        <tr>
            <td><%=trago.nombre%></td>
            <td><%=trago.gradosAlcohol%></td>
            <td><%=trago.fechaCaducidad%></td>
            <td><%=trago.precio%></td>
            <td><%=trago.tipo%></td>
            <td>
                <botton>editar</botton>
                <botton>eliminar</botton>
            </td>
        </tr>
    <%})%>
</table>
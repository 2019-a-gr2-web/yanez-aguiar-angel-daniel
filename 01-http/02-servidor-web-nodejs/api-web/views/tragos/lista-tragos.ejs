<style>
    table,tr,td,th{
        border: solid black 1px;
    }
</style>

<a href="/api/traguito/crear">
    <button>crear</button>
</a>

<table>
    <tr>
        <th>Nombre</th>
        <th>Grados</th>
        <th>Caduca</th>
        <th>Precio</th>
        <th>Tipo</th>
        <th>Acciones</th>
    </tr>
    <%arregloTragos.forEach((trago)=>{%>
        <tr>
            <td><%=trago.nombre%></td>
            <td><%=trago.gradosAlcohol%></td>
            <td><%=trago.fechaCaducidad%></td>
            <td><%=trago.precio%></td>
            <td><%=trago.tipo%></td>
            <td>
                <button>editar</button>
                <form action="/api/traguito/eliminar" method="post">
                    <input type="hidden" name="id" value="<%=trago.id%>"/>
                    <button>eliminar</button>
                </form>
            </td>
        </tr>
    <%})%>
</table>